[
    {
        "name": "bash",
        "file_pattern": "\\\\.(sh|bash)$",
        "module": "bash",
        "strided": false,
        "queries": [
            {
                "name": "functions",
                "query": [
                    "(function_definition) @capture"
                ]
            }
        ]
    },
    {
        "name": "protobuf",
        "file_pattern": "\\\\.proto$",
        "module": "protobuf",
        "strided": true,
        "queries": [
            {
                "name": "functions",
                "query": [
                    "(service) @service"
                ]
            }
        ]
    },
    {
        "name": "python",
        "file_pattern": "\\\\.py$",
        "module": "python",
        "strided": false,
        "queries": [
            {
                "name": "function",
                "query": [
                    "(function_definition) @capture"
                ]
            }
        ]
    },
    {
        "name": "golang",
        "file_pattern": "\\\\.go",
        "module": "golang",
        "strided": false,
        "queries": [
            {
                "name": "function",
                "query": [
                    "[",
                    "    (function_declaration),",
                    "    (method_declaration),",
                    "    (func_literal)",
                    "] @ function"
                ]
            }
        ]
    }
]
