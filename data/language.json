{
    "bash": {
        "file_pattern": "\\.(sh|bash)$",
        "strided": true,
        "queries": [
            {
                "name": "functions",
                "query": [
                    "(function_declaration",
                    "name: (identifier) @function.name",
                    "body: (block) @function.body)"
                ]
            },
        ]
    },
